                                    ]
                                  },
                                  {
                                    "required": [
                                      "schema:url"
                                    ]
                                  }
                                ]
                              }
                            ]
                          },
                          {
                            "type": "string"
                          }
                        ]
                      },
                      "schema:sameAs": {
                        "type": "array",
                        "description": "other identifiers for the organization",
                        "items": {
                          "type": "string"
                        }
                      }
                    },
                    "allOf": [
                      {
                        "required": [
                          "@type"
                        ]
                      },
                      {
                        "anyOf": [
                          {
                            "required": [
                              "schema:name"
                            ]
                          },
                          {
                            "required": [
                              "schema:identifier"
                            ]
                          }
                        ]
                      }
                    ],
                    "description": "if affiliation is present, value must be a schema:Organization."
                  },
                  "schema:contactPoint": {
                    "type": "object",
                    "properties": {
                      "@type": {
                        "type": "string",
                        "const": "schema:ContactPoint",
                        "default": "schema:ContactPoint"
                      },
                      "schema:email": {
                        "type": "string"
                      }
                    },
                    "description": "restrict to email only. Schema.org allows telephone and postal contacts as well"
                  },
                  "schema:sameAs": {
                    "type": "array",
                    "description": "other identifiers for the person",
                    "items": {
                      "type": "string"
                    }
                  }
                },
                "allOf": [
                  {
                    "required": [
                      "@type"
                    ]
                  },
                  {
                    "anyOf": [
                      {
                        "required": [
                          "schema:name"
                        ]
                      },
                      {
                        "required": [
                          "schema:identifier"
                        ]
                      }
                    ]
                  }
                ]
              },
              {
                "type": "object",
                "properties": {
                  "@id": {
                    "type": "string"
                  },
                  "@type": {
                    "type": "string",
                    "const": "schema:Organization",
                    "default": "schema:Organization"
                  },
                  "schema:additionalType": {
                    "type": "array",
                    "items": {
                      "anyOf": [
